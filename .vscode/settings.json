{
	"json.schemas": [
		{
			"fileMatch": [
				"/can_bus/json/**/*tx.json"
			],
			"url": "/can_bus/json/schema_tx.json"
		}
	],
	"cmake.buildDirectory": "${workspaceFolder}/${env:BUILD_DIR}",
	"cmake.configureOnEdit": false,
	"cmake.configureOnOpen": true,
	"files.associations": {
		"*.c": "c",
		"*.html": "html",
		"*_repl": "cpp",
		"*.bpred": "cpp",
		"array": "c",
		"string_view": "c",
		"initializer_list": "c",
		"ranges": "c",
		"utility": "c",
		"*.tcc": "c"
	},
}