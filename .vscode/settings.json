{
	"json.schemas": [
		{
			"fileMatch": [
				"/can_bus/json/**/*tx.json"
			],
			"url": "/can_bus/json/schema_tx.json"
		}
	],
	"cmake.configureOnEdit": false,
	"cmake.configureOnOpen": false,
	"files.associations": {
		"main.h": "c",
		"hw_bootup.h": "c",
		"hw_pwms.h": "c",
		"hw_hardfaulthandler.h": "c",
		"hw_watchdog.h": "c",
		"hw_pwminputfreqonly.h": "c",
		"io_leds.h": "c",
		"io_led.h": "c",
		"hw_adcs.h": "c",
		"hw_gpios.h": "c"
	},
	"clangd.arguments": [
		"--compile-commands-dir=build_fw_dev",
 		// "--compile-commands-dir=build_fw_test",
		"--query-driver=/usr/local/bin/arm-none-eabi-gcc",
	]
}