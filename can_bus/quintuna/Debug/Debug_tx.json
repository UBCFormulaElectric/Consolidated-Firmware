{
  "CanMode": {
    "msg_id": 1001,
    "cycle_time": 1000,
    "description": "Signals related to CAN modes.",
    "signals": {
      "EnableDebugMode": {
        "bits": 1
      }
    }
  },
  "Charging": {
    "msg_id": 1002,
    "cycle_time": 100,
    "description": "Value set by user to start charging BMS.",
    "signals": {
      "StartCharging": {
        "bits": 1
      },
      "ChargingCurrent": {
        "resolution": 0.1,
        "min": 0,
        "max": 14,
        "unit": "A"
      }
    }
  },
  "CellBalancing": {
    "msg_id": 1003,
    "cycle_time": 1000,
    "description": "Signals related to BMS cell balancing.",
    "signals": {
      "CellBalancingRequest": {
        "bits": 1
      },
      "CellBalancingOverrideTarget": {
        "bits": 1
      },
      "CellBalancingOverrideTargetValue": {
        "resolution": 0.0001,
        "min": 3,
        "max": 4.2,
        "unit": "V"
      },
      "CellBalancingOverridePWM": {
        "bits": 1
      },
      "CellBalancingOverridePWMFrequency": {
        "resolution": 0.1,
        "min": 0,
        "max": 100,
        "unit": "Hz"
      },
      "CellBalancingOverridePWMDuty": {
        "resolution": 1,
        "min": 0,
        "max": 100,
        "unit": "%"
      }
    }
  },
  "ResetSoc": {
    "msg_id": 1004,
    "cycle_time": 100,
    "description": "Value set by user to set SOC based on min cell voltage.",
    "signals": {
      "ResetSoc_MinCellV": {
        "bits": 1
      },
      "ResetSoc_CustomEnable": {
        "bits": 1
      },
      "ResetSoc_CustomVal": {
        "resolution": 0.1,
        "min": 0,
        "max": 100,
        "unit": "%"
      }
    }
  },
  "SetCoolantPump": {
    "msg_id": 1005,
    "cycle_time": 1000,
    "description": "Value set by the user to set the coolant pump flow rate.",
    "signals": {
      "SetCoolantPump_CustomEnable": {
        "bits": 1
      },
      "SetCoolantPump_CustomVal": {
        "resolution": 15.625,
        "min": 0,
        "max": 2000,
        "unit": "L/H"
      }
    }
  },
  "SetBSPDTestCurrent": {
    "msg_id": 1006,
    "cycle_time": 100,
    "description": "Enable BSPD Test Current",
    "signals": {
      "EnableTestCurrent": {
        "bits": 1
      }
    }
  },
  "GearBoxBreaking": {
    "msg_id": 1007,
    "cycle_time": 100,
    "description": "Enable Gearbox Breakin Mode",
    "signals": {
      "EnableBreakinMode": {
        "bits": 1
      },
      "VelocitySetpoint": {
        "resolution": 0.1,
        "min": -18000,
        "max": 18000,
        "unit": "1/rpm"}
    }
  }
}