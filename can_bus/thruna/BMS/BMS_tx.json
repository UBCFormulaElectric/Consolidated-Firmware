{
    "Vitals": {
        "msg_id": 100,
        "cycle_time": 100,
        "description": "Vitals message for the BMS.",
        "signals": {
            "Heartbeat": {
                "bits": 1
            },
            "State": {
                "enum": "BmsState"
            }            
        }
    },
    "AlertsContext": {
        "msg_id": 104,
        "cycle_time": 1000,
        "description": "Context info for BMS alerts",
        "signals": {
            "TxOverflowCount": {
                "bits": 8
            },
            "RxOverflowCount": {
                "bits": 8
            },
            "WatchdogTimeoutTaskName": {
                "enum": "RtosTaskName"
            }
        }
    },
    "ImdStatus": {
        "msg_id": 105,
        "cycle_time": 1000,
        "description": "BMS IMD Messages",
        "signals": {
            "ImdOkHs": {
                "bits": 1
            },
            "ImdSecondsSincePowerOn": {
                "bits": 8,
                "min": 0,
                "max": 255,
                "unit": "Seconds"
            },
            "ImdValidDutyCycle": {
                "bits": 1
            },
            "ImdCondition": {
                "enum": "ImdConditionName"
            },
            "ImdDutyCycle": {
                "bits": 8,
                "min": 0,
                "max": 100,
                "unit": "%"
            },
            "ImdFrequency": {
                "bits": 8,
                "min": 0,
                "max": 100,
                "unit": "Hz"
            }
        }
    },
    "ImdData": {
        "msg_id": 106,
        "cycle_time": 1000,
        "description": "BMS IMD Measurement Data Values",
        "signals": {
            "ImdActiveFrequency": {
                "enum": "ImdActiveFrequency"
            },
            "ImdInsulationMeasurementDcp10Hz": {
                "resolution": 0.1,
                "min": 0,
                "max": 65535,
                "unit": "kOhm"
            },
            "ImdInsulationMeasurementDcp20Hz": {
                "resolution": 0.1,
                "min": 0,
                "max": 65535,
                "unit": "kOhm"
            },
            "ImdSpeedStartStatus30Hz": {
                "resolution": 0.1,
                "min": 0,
                "max": 65535
            }
        }
    },

