{
    "BMS_Vitals": {
        "msg_id": 100,
        "cycle_time": 100,
        "description": "Vitals message for the BMS.",
        "signals": {
            "Heartbeat": {
                "bits": 1
            },
            "CurrentState": {
                "enum": "BmsState"
            },
            "StateOfCharge": {
                "bits": 3,
                "min": 0,
                "max": 100
            }
        }
    },
    "BMS_AlertsContext": {
        "msg_id": 104,
        "cycle_time": 1000,
        "description": "Context info for BMS alerts",
        "signals": {
            "TxOverflowCount": {
                "bits": 8
            },
            "RxOverflowCount": {
                "bits": 8
            },
            "WatchdogTimeoutTaskName": {
                "enum": "RtosTaskName"
            }
        }
    },
    "BMS_ImdStatus": {
        "msg_id": 105,
        "cycle_time": 1000,
        "description": "BMS IMD Messages",
        "signals": {
            "OkHs": {
                "bits": 1
            },
            "SecondsSincePowerOn": {
                "bits": 8,
                "min": 0,
                "max": 255,
                "unit": "Seconds"
            },
            "ValidDutyCycle": {
                "bits": 1
            },
            "Condition": {
                "signed": false,
                "bits": 4
            }
        }
    },
    "BMS_ImdData": {
        "msg_id": 106,
        "cycle_time": 1000,
        "description": "BMS IMD Measurement Data Values",
        "signals": {
            "ActiveFrequency": {
                "enum": "ImdActiveFrequency"
            },
            "InsulationMeasurementDcp10Hz": {
                "resolution": 0.1,
                "min": 0,
                "max": 65535,
                "unit": "kOhm"
            },
            "InsulationMeasurementDcp20Hz": {
                "resolution": 0.1,
                "min": 0,
                "max": 65535,
                "unit": "kOhm"
            },
            "SpeedStartStatus30Hz": {
                "resolution": 0.1,
                "min": 0,
                "max": 65535
            }
        }
    },
    "BMS_ImdPwmOutput": {
        "msg_id": 107,
        "cycle_time": 1000,
        "description": "BMS IMD PWM Output",
        "signals": {
            "DutyCycle": {
                "bits": 8,
                "min": 0,
                "max": 100,
                "unit": "%"
            },
            "Frequency": {
                "bits": 8,
                "min": 0,
                "max": 100,
                "unit": "Hz"
            }
        }
    },
    "BMS_Charger": {
        "msg_id": 110,
        "cycle_time": 1000,
        "description": "Flags relaying BMS Charger Status",
        "signals": {
            "IsConnected": {
                "bits": 1
            },
            "IsChargingComplete": {
                "bits": 1
            }
        }
    },
    "BMS_OkStatuses": {
        "msg_id": 111,
        "cycle_time": 10,
        "description": "BMS OK status flags",
        "signals": {
            "BmsOk": {
                "bits": 1
            },
            "ImdOk": {
                "bits": 1
            },
            "BspdOk": {
                "bits": 1
            },
            "ImdLatchedFaultStatus": {
                "bits": 1
            },
            "BspdLatchedFaultStatus": {
                "bits": 1
            }
        }
    },
    "BMS_Contactors": {
        "msg_id": 112,
        "cycle_time": 10,
        "description": "BMS AIR state statuses",
        "signals": {
            "AirPositive": {
                "enum": "ContactorState"
            },
            "AirNegative": {
                "enum": "ContactorState"
            },
            "PrechargeRelay": {
                "enum": "ContactorState"
            }
        }
    },
    "BMS_MotorShutdownErrors": {
        "msg_id": 113,
        "cycle_time": 1000,
        "description": "BMS Motor Shutdown Errors",
        "signals": {
            "BmsDummyMotorShutdown": {
                "bits": 1
            }
        }
    },
    "BMS_CellTemperatures": {
        "msg_id": 114,
        "cycle_time": 100,
        "description": "BMS Cell Temperature readings",
        "signals": {
            "MinCellTemperature": {
                "resolution": 0.1,
                "min": 0,
                "max": 100,
                "unit": "degC"
            },
            "MaxCellTemperature": {
                "resolution": 0.1,
                "min": 0,
                "max": 100,
                "unit": "degC"
            }
        }
    },
    "BMS_CellVoltages": {
        "msg_id": 115,
        "cycle_time": 100,
        "description": "BMS Cell Voltage readings",
        "signals": {
            "MinCellVoltage": {
                "resolution": 0.0001,
                "min": 0,
                "max": 5,
                "unit": "V"
            },
            "MaxCellVoltage": {
                "resolution": 0.0001,
                "min": 0,
                "max": 5,
                "unit": "V"
            }
        }
    },
    "BMS_CellStats": {
        "msg_id": 116,
        "cycle_time": 100,
        "description": "BMS Cell Stats",
        "signals": {
            "MinCellVoltageSegment": {
                "resolution": 1,
                "min": 0,
                "max": 7
            },
            "MinCellVoltageIdx": {
                "resolution": 1,
                "min": 0,
                "max": 15
            },
            "MaxCellVoltageSegment": {
                "bits": 3,
                "min": 0,
                "max": 7
            },
            "MaxCellVoltageIdx": {
                "resolution": 1,
                "min": 0,
                "max": 15
            },
            "MinTempSegment": {
                "resolution": 1,
                "min": 0,
                "max": 7
            },
            "MinTempIdx": {
                "resolution": 1,
                "min": 0,
                "max": 15
            },
            "MaxTempSegment": {
                "resolution": 1,
                "min": 0,
                "max": 7
            },
            "MaxTempIdx": {
                "resolution": 1,
                "min": 0,
                "max": 15
            }
        }
    },
    "BMS_TractiveSystem": {
        "msg_id": 117,
        "cycle_time": 100,
        "description": "BMS Tractive System Data",
        "signals": {
            "TsVoltage": {
                "resolution": 0.0001,
                "min": 0,
                "max": 600,
                "unit": "V"
            },
            "TsCurrent": {
                "resolution": 0.1,
                "min": -300,
                "max": 300,
                "unit": "A"
            }
        }
    },
    "BMS_PackVoltage": {
        "msg_id": 118,
        "cycle_time": 100,
        "description": "BMS Pack Voltage",
        "signals": {
            "PackVoltage": {
                "resolution": 0.0001,
                "min": 0,
                "max": 600,
                "unit": "V"
            }
        }
    },
    "BMS_AvailablePower": {
        "msg_id": 120,
        "cycle_time": 100,
        "description": "BMS Available Power",
        "signals": {
            "AvailablePower": {
                "resolution": 0.1,
                "min": 0,
                "max": 100000,
                "unit": "W"
            }
        }
    },
