{
    "Vitals": {
        "msg_id": 100,
        "cycle_time": 100,
        "description": "Vitals message for the BMS.",
        "signals": {
            "Heartbeat": {
                "bits": 1
            },
            "State": {
                "enum": "BmsState"
            }
        }
    },
    "AlertsContext": {
        "msg_id": 104,
        "cycle_time": 1000,
        "description": "Context info for BMS alerts",
        "signals": {
            "TxOverflowCount": {
                "bits": 16
            },
            "RxOverflowCount": {
                "bits": 16
            },
            "WatchdogTimeoutTaskName": {
                "enum": "RtosTaskName"
            },
            "ModuleCommunication_NumCommTries": {
                "bits": 16
            },
            "ModuleCommunication_MonitorState": {
                "enum": "CAN_AccumulatorMonitorState"
            }
        }
    },
    "ImdStatus": {
        "msg_id": 105,
        "cycle_time": 100,
        "description": "BMS IMD Messages",
        "signals": {
            "ImdOkHs": {
                "bits": 1
            },
            "ImdTimeSincePowerOn": {
                "min": 0,
                "resolution": 1,
                "max": 10000,
                "unit": "ms"
            },
            "ImdValidDutyCycle": {
                "bits": 1
            },
            "ImdCondition": {
                "enum": "ImdConditionName"
            },
            "ImdDutyCycle": {
                "bits": 8,
                "min": 0,
                "max": 100,
                "unit": "%"
            },
            "ImdFrequency": {
                "bits": 8,
                "min": 0,
                "max": 100,
                "unit": "Hz"
            }
        }
    },
    "ImdData": {
        "msg_id": 106,
        "cycle_time": 1000,
        "description": "BMS IMD Measurement Data Values",
        "signals": {
            "ImdActiveFrequency": {
                "enum": "ImdActiveFrequency"
            },
            "ImdInsulationMeasurementDcp10Hz": {
                "resolution": 0.1,
                "min": 0,
                "max": 65535,
                "unit": "kOhm"
            },
            "ImdInsulationMeasurementDcp20Hz": {
                "resolution": 0.1,
                "min": 0,
                "max": 65535,
                "unit": "kOhm"
            },
            "ImdSpeedStartStatus30Hz": {
                "resolution": 0.1,
                "min": 0,
                "max": 65535
            }
        }
    },
    "Charger": {
        "msg_id": 107,
        "cycle_time": 100,
        "description": "Charger infomation for the BMS to broadcast to other boards",
        "signals": {
            "ChargerConnected": {
                "bits": 1
            }
        }
    },
    "BrusaControls": {
        "msg_id": 1560,
        "cycle_time": 100,
        "description": "Flags and parameters for controlling the BRUSA charger",
        "signals": {
            "ChargerEnable": {
                "start_bit": 7,
                "bits": 1
            },
            "ClearLatch": {
                "start_bit": 6,
                "bits": 1
            },
            "MaxMainsCurrent": {
                "start_bit": 8,
                "min": 0,
                "max": 6000,
                "resolution": 0.1
            },
            "ChargingVoltage": {
                "start_bit": 24,
                "min": 0,
                "max": 6000,
                "resolution": 0.1
            },
            "ChargingCurrent": {
                "start_bit": 40,
                "min": 0,
                "max": 6000,
                "resolution": 0.1
            }
        }
    },
    "OkStatuses": {
        "msg_id": 111,
        "cycle_time": 100,
        "description": "BMS OK status flags",
        "signals": {
            "BmsOk": {
                "bits": 1
            },
            "ImdOk": {
                "bits": 1
            },
            "BspdOk": {
                "bits": 1
            },
            "BmsLatchedFault": {
                "bits": 1
            },
            "ImdLatchedFault": {
                "bits": 1
            },
            "BspdLatchedFault": {
                "bits": 1
            }
        }
    },
    "Contactors": {
        "msg_id": 112,
        "cycle_time": 100,
        "description": "BMS AIR state statuses",
        "signals": {
            "AirPositive": {
                "enum": "ContactorState"
            },
            "AirNegative": {
                "enum": "ContactorState"
            },
            "PrechargeRelay": {
                "enum": "ContactorState"
            }
        }
    },
    "CellTemperatures": {
        "msg_id": 120,
        "cycle_time": 1000,
        "description": "BMS Cell Temperature readings",
        "signals": {
            "MinCellTemperature": {
                "resolution": 0.1,
                "min": 0,
                "max": 100,
                "unit": "degC"
            },
            "MaxCellTemperature": {
                "resolution": 0.1,
                "min": 0,
                "max": 100,
                "unit": "degC"
            }
        }
    },
    "VoltageAndChargeStats": {
        "msg_id": 121,
        "cycle_time": 100,
        "description": "BMS Cell Voltage readings",
        "signals": {
            "PackVoltage": {
                "resolution": 0.01,
                "min": 0,
                "max": 600,
                "unit": "V"
            },
            "Soc": {
                "resolution": 0.1,
                "min": 0,
                "max": 100,
                "unit": "%"
            },
            "SocCorrupt": {
                "bits": 1
            },
            "MinCellVoltage": {
                "resolution": 0.001,
                "min": 0,
                "max": 5,
                "unit": "V"
            },
            "MaxCellVoltage": {
                "resolution": 0.001,
                "min": 0,
                "max": 5,
                "unit": "V"
            }
        }
    },
    "CellStats": {
        "msg_id": 123,
        "cycle_time": 100,
        "description": "BMS Cell Stats",
        "signals": {
            "MinCellVoltageSegment": {
                "resolution": 1,
                "min": 0,
                "max": 7
            },
            "MinCellVoltageIdx": {
                "resolution": 1,
                "min": 0,
                "max": 15
            },
            "MaxCellVoltageSegment": {
                "bits": 3,
                "min": 0,
                "max": 7
            },
            "MaxCellVoltageIdx": {
                "resolution": 1,
                "min": 0,
                "max": 15
            },
            "MinTempSegment": {
                "resolution": 1,
                "min": 0,
                "max": 7
            },
            "MinTempIdx": {
                "resolution": 1,
                "min": 0,
                "max": 15
            },
            "MaxTempSegment": {
                "resolution": 1,
                "min": 0,
                "max": 7
            },
            "MaxTempIdx": {
                "resolution": 1,
                "min": 0,
                "max": 15
            }
        }
    },
    "TractiveSystem": {
        "msg_id": 130,
        "cycle_time": 100,
        "description": "BMS Tractive System Data",
        "signals": {
            "TractiveSystemVoltage": {
                "resolution": 0.1,
                "min": 0,
                "max": 600,
                "unit": "V"
            },
            "TractiveSystemCurrent": {
                "resolution": 0.1,
                "min": -300,
                "max": 300,
                "unit": "A"
            },
            "TractiveSystemPower": {
                "resolution": 0.1,
                "min": -100,
                "max": 100,
                "unit": "kW"
            },
            "AvailablePower": {
                "resolution": 1,
                "min": 0,
                "max": 100000,
                "unit": "W"
            },
            "BSPDCurrentThresholdExceeded": {
                "bits": 1
            }
        }
    },
    "CommitInfo": {
        "msg_id": 140,
        "cycle_time": 100,
        "description": "BMS Current Running Git Commit Info",
        "signals": {
            "Hash": {
                "bits": 32
            },
            "Clean": {
                "bits": 1
            }
        }
    },
