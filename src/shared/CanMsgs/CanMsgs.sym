Var=CELL_BALANCE_OVERVOLTAGE_FAULT bit 2,1 /ln:"TODO: What does this mean?"
Var=CHARGER_FAULT bit 3,1 /ln:"TODO: What does this mean?"
Var=EFUSE_FAULT bit 4,1 /ln:"TODO: What does this mean?"
Var=_12V_FAULT_UNDER_VOLTAGE bit 5,1 /ln:"TODO: What does this mean?"
Var=_12V_FAULT_OVER_VOLTAGE bit 6,1 /ln:"TODO: What does this mean?"
Var=VBAT_FAULT bit 7,1 /ln:"TODO: What does this mean?"

[PDM_CAN_TX_FIFO_OVERFLOW]
ID=062h	// Indicates that we tried to transmit messages faster then we can send them, so our internal FIFO queue overflowed
Len=4
Var=overflow_count unsigned 0,32 /max:1

[PDM_HEARTBEAT]
ID=061h	// TODO: descriptive comment
