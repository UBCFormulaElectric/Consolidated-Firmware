{
//    BO_ 100 BMS_VITALS: 1 BMS
//    SG_ HEARTBEAT : 0|1@1+ (1,0) [0|1] "" FSM,DCM,PDM,DIM,GSM
    "BMSVitals": {
        "msg_id": 100,
        "cycle_time": 1000,
        "description": "Vitals message for the BMS.",
        "signals": {
            "Heartbeat": {
                "bits": 1
            },
            "CurrentState":{
                "enum" : "BmsState"
            }
        }
    },

//    BO_ 101 BMS_CAN_FIFO_OVERFLOW: 8 BMS
//    SG_ tx_overflow_count : 0|32@1+ (1,0) [0|4294967295] "" DEBUG
//    SG_ rx_overflow_count : 32|32@1+ (1,0) [0|4294967295] "" DEBUG

    "BMSCanFifoOverflow": {
        "msg_id": 101,
        "cycle_time": 1000,
        "description": "BMS tx/rx fifo overflow count",
        "signals": {
            "TxOverflowCount": {
                "bits": 32
            },
            "RxOverflowCount": {
                "bits": 32
            }
        }
    },

//    BO_ 103 BMS_WATCHDOG_TIMEOUT : 8 BMS
//    SG_ TASK_NAME : 0|64@1+ (1,0) [0|0] "" DEBUG

    "BMSWatchdogTimeout": {
        "msg_id": 102,
        "cycle_time": 1000,
        "description": "",
        "signals": {
            "TaskName" : {
                "enum": "SharedTasks"
            }
        }
    },

//    BO_ 104 BMS_WARNINGS: 8 BMS
//    SG_ STACK_WATERMARK_ABOVE_THRESHOLD_TASK1HZ : 0|1@1+ (1,0) [0|1] "" DIM
//    SG_ STACK_WATERMARK_ABOVE_THRESHOLD_TASK1KHZ : 1|1@1+ (1,0) [0|1] "" DIM
//    SG_ STACK_WATERMARK_ABOVE_THRESHOLD_TASKCANRX : 2|1@1+ (1,0) [0|1] "" DIM
//    SG_ STACK_WATERMARK_ABOVE_THRESHOLD_TASKCANTX : 3|1@1+ (1,0) [0|1] "" DIM
//    SG_ WATCHDOG_TIMEOUT : 4|1@1+ (1,0) [0|1] "" DIM

    "BMSWarnings": {
        "msg_id" : 103,
        "cycle_time":  1000,
        "description": "Warning flags for BMS",
        "signals": {
            "StackWatermarkAboveThresholdTask1HZ": {
                "bits": 1
            },
            "StackWatermarkAboveThresholdTask1KHZ": {
                "bits": 1
            },
            "StackWatermarkAboveThresholdTaskCanRx": {
                "bits": 1
            },
            "StackWatermarkAboveThresholdTaskCanTx": {
                "bits": 1
            },
            "WatchdogTimeout": {
                "bits": 1
            }
        }
    },

//    BO_ 105 BMS_IMD: 8 BMS
//    SG_ Condition M : 0|3@1+ (1,0) [0|6] "" DEBUG
//    SG_ Seconds_Since_Power_On : 3|16@1+ (1,0) [0|255] "Seconds" DEBUG
//    SG_ Valid_Duty_Cycle : 19|1@1+ (1,0) [0|1] "" DEBUG
//    SG_ Insulation_Measurement_DCP_10Hz m1 : 20|16@1+ (1,0) [0|65535] "kOhm" DEBUG
//    SG_ Insulation_Measurement_DCP_20Hz m2 : 20|16@1+ (1,0) [0|65535] "kOhm" DEBUG
//    SG_ Speed_Start_Status_30Hz m3 : 20|16@1+ (1,0) [0|65535] "" DEBUG
//    SG_ OK_HS : 36|1@1+ (1,0) [0|1] "" DIM

    "BmsImd": {
            "msg_id": 104,
            "cycle_time": 1000,
            "description": "BMS IMD Messages",
            "signals": {
                "SecondsSincePowerOn" : {
                    "bits": 8,
                    "unit": "Seconds"
                },
                "ValidDutyCycle": {
                    "bits": 1
                },
                "InsulationMeasurementDcp10Hz" : {
                    "resolution": 0.1,
                    "min" : 0,
                    "max": 65535,
                    "unit": "kOhm"
                },
                "Insulation_Measurement_DCP_20Hz" :{
                    "resolution": 0.1,
                    "min": 0,
                    "max": 65535,
                    "unit": "kOhm"
                },
                "SpeedStartStatus30Hz":{
                    "resolution": 0.1,
                    "min": 0,
                    "max": 65535
                },
                "OkHs": {
                    "bits": 1
                }
            }
        },

//        BO_ 110 BMS_CHARGER: 1 BMS
//        SG_ Is_Connected : 0|1@1+ (1,0) [0|1] "" DEBUG
//    SG_ IS_CHARGING_COMPLETE : 1|1@1+ (1,0) [0|1] "" DEBUG
//

    "BmsCharger": {
        "msg_id": 110,
        "cycle_time": 1000,
        "description": "Flags relaying BMS Charger Status",
        "signals": {
            "IsConnected": {
                "bits": 1
            },
            "IsChargingComplete": {
                "bits": 1
            }
        }
    },

//    BO_ 111 BMS_OK_STATUSES: 1 BMS
//    SG_ BMS_OK : 0|1@1+ (1,0) [0|1] "" DIM
//    SG_ IMD_OK : 1|1@1+ (1,0) [0|1] "" DIM
//    SG_ BSPD_OK : 2|1@1+ (1,0) [0|1] "" DIM
//
    "BmsOkStatuses": {
        "msg_id": 111,
        "cycle_time": 1000,
        "description": "BMS OK status flags",
        "signals": {
            "BmsOk": {
                "bits": 1
            },
            "ImdOk":{
                "bits": 1
            },
            "BspdOk": {
                "bits": 1
            }
        }
    },
//    BO_ 112 BMS_AIR_STATES: 1 BMS
//    SG_ AIR_POSITIVE : 0|1@1+ (1,0) [0|1] "" FSM,DCM,PDM
//    SG_ AIR_NEGATIVE : 1|1@1+ (1,0) [0|1] "" FSM,DCM,PDM
//    SG_ PRECHARGE_RELAY: 2|1@1+ (1,0) [0|1] "" DEBUG
//
    "BmsAirStates": {
        "msg_id": 112,
        "cycle_time": 1000,
        "description": "BMS AIR state statuses",
        "signals": {
            "AirPositive": {
                "bits": 1
            },
            "AirNegative":{
                "bits": 1
            },
            "PrechargeRelay": {
                "bits": 1
            }
        }
    },
//    BO_ 113 BMS_MOTOR_SHUTDOWN_ERRORS: 8 BMS
//    SG_ DUMMY_MOTOR_SHUTDOWN : 0|1@1+ (1,0) [0|1] "" DCM
//
    "BmsMotorShutdownErrors": {
        "msg_id": 113,
        "cycle_time": 1000,
        "description": "BMS Motor Shutdown Errors",
        "signals": {
            "BmsDummyMotorShutdown": {
                "bits": 1
            }
        }
    },
//    BO_ 114 BMS_CELL_TEMPERATURES: 8 BMS
//    SG_ MIN_CELL_TEMPERATURE: 0|32@1+ (1,0) [0|100] "degC" DEBUG
//    SG_ MAX_CELL_TEMPERATURE: 32|32@1+ (1,0) [0|100] "degC" DEBUG
//

    "BmsCellTempuratures": {
        "msg_id": 114,
        "cycle_time": 1000,
        "description": "BMS Cell Temperature readings",
        "signals": {
            "MinCellTemperature": {
                "resolution": 0.1,
                "min": 0,
                "max": 100,
                "unit": "degC"
            },
            "MaxCellTemperature": {
                "resolution": 0.1,
                "min": 0,
                "max": 100,
                "unit": "degC"
            }
        }
    },
//    BO_ 115 BMS_CELL_VOLTAGES: 8 BMS
//    SG_ MIN_CELL_VOLTAGE : 0|32@1+ (1,0) [0|5] "" DEBUG
//    SG_ MAX_CELL_VOLTAGE : 32|32@1+ (1,0) [0|5] "" DEBUG
//
    "BmsCellVoltages": {
        "msg_id": 115,
        "cycle_time": 1000,
        "description": "BMS Cell Voltage readings",
        "signals": {
            "MinCellVoltage": {
                "resolution": 0.0001,
                "min": 0,
                "max": 5,
                "unit": "V"
            },
            "MaxCellVoltage": {
                "resolution": 0.0001,
                "min": 0,
                "max": 5,
                "unit": "V"
            }
        }
    },

//    BO_ 116 BMS_CELL_STATS: 4 BMS
//    SG_ MIN_CELL_VOLTAGE_SEGMENT : 0|3@1+ (1,0) [0|7] "" DEBUG
//    SG_ MIN_CELL_VOLTAGE_IDX : 4|4@1+ (1,0) [0|15] "" DEBUG
//    SG_ MAX_CELL_VOLTAGE_SEGMENT : 8|3@1+ (1,0) [0|7] "" DEBUG
//    SG_ MAX_CELL_VOLTAGE_IDX : 11|4@1+ (1,0) [0|7] "" DEBUG
//    SG_ MIN_TEMP_SEGMENT : 15|3@1+ (1,0) [0|7] "" DEBUG
//    SG_ MIN_TEMP_IDX : 18|4@1+ (1,0) [0|15] "" DEBUG
//    SG_ MAX_TEMP_SEGMENT : 22|3@1+ (1,0) [0|7] "" DEBUG
//    SG_ MAX_TEMP_IDX : 25|4@1+ (1,0) [0|7] "" DEBUG
//

    "BmsCellStats": {
        "msg_id": 116,
        "cycle_time": 1000,
        "description": "BMS Cell Stats",
        "signals": {
            "MinCellVoltageSegment": {
                "bits": 3,
                "resolution": 1,
                "min": 0,
                "max": 7
            },
            "MinCellVoltageIdx": {
                "bits": 4,
                "resolution": 1,
                "min": 0,
                "max": 15
            },
            "MaxCellVoltageSegment": {
                "bits": 3,
                "min": 0,
                "max": 7
            },
            "MaxCellVoltageIdx":{
                "bits": 4,
                "resolution": 1,
                "min": 0,
                "max": 15
            },
            "MinTempSegment": {
                "bits": 3,
                "resolution": 1,
                "min": 0,
                "max": 7
            },
            "MinTempIdx": {
                "bits": 4,
                "resolution": 1,
                "min": 0,
                "max": 15
            },
            "MaxTempSegment": {
                "bits": 3,
                "resolution": 1,
                "min": 0,
                "max": 7
            },
            "MaxTempIdx":{
                "bits": 4,
                "resolution": 1,
                "min": 0,
                "max": 15
            }
        }
    },
//    BO_ 117 BMS_TRACTIVE_SYSTEM: 8 BMS
//    SG_ TS_VOLTAGE : 0|32@1+ (1,0) [0|600] "" DEBUG
//    SG_ TS_CURRENT : 32|32@1+ (1,0) [-300|300] "" DEBUG ** + indicates unsigned but has negative range????
//
    "BmsTractiveSystem": {
        "msg_id": 117,
        "cycle_time": 1000,
        "description": "BMS Tractive System Data",
        "signals": {
            "TsVoltage": {
                "resolution": 0.0001,
                "min": 0,
                "max": 600,
                "unit": "V"
            },
            "TsCurrent": {
                "resolution": 0.1,
                "signed": true,
                "min": 0,
                "max": 600,
                "unit": "A"
            }
        }
    },

//    BO_ 118 BMS_PACK_VOLTAGE: 4 BMS
//    SG_ PACK_VOLTAGE : 0|32@1+ (1,0) [0|600] "" DEBUG
//
    "BmsPackVoltage": {
        "msg_id": 118,
        "cycle_time": 1000,
        "description": "BMS Pack Voltage",
        "signals": {
            "PackVoltage": {
                "resolution": 0.0001,
                "min": 0,
                "max": 600,
                "unit": "V",
            }
        }
    },

//    BO_ 120 BMS_AVAILABLE_POWER: 4 BMS
//    SG_ AVAILABLE_POWER : 0|32@1+ (1,0) [0|100000] "W" DCM
    "BmsAvailablePower": {
        "msg_id": 120,
        "cycle_time": 1000,
        "description": "BMS Available Power",
        "signals": {
            "AvailablePower": {
                "resolution": 0.1,
                "min": 0,
                "max": 100000,
                "unit": "W"
            }
        }
    }
}
