{
    "BMSVitals": {
        "msg_id": 100,
        "cycle_time": 100,
        "description": "Vitals message for the BMS.",
        "signals": {
            "Heartbeat": {
                "bits": 1
            },
            "CurrentState":{
                "enum" : "BMSState"
            },
            "StateOfCharge": {
                "bits": 3,
                "min": 0,
                "max": 100
            }
        }
    },

    "BMSCanFifoOverflow": {
        "msg_id": 101,
        "cycle_time": 5000,
        "description": "BMS tx/rx fifo overflow count",
        "signals": {
            "TxOverflowCount": {
                "bits": 32
            },
            "RxOverflowCount": {
                "bits": 32
            }
        }
    },

    "BMSWatchdogTimeout": {
        "msg_id": 103,
        "cycle_time": 1000,
        "description": "placeholder",
        "signals": {
            "TaskName" : {
                "enum": "SharedTasks"
            }
        }
    },

    "BMSWarnings": {
        "msg_id" : 104,
        "cycle_time":  1000,
        "description": "Warning flags for BMS",
        "signals": {
            "StackWatermarkAboveThresholdTask1Hz": {
                "bits": 1
            },
            "StackWatermarkAboveThresholdTask1kHz": {
                "bits": 1
            },
            "StackWatermarkAboveThresholdTaskCanRx": {
                "bits": 1
            },
            "StackWatermarkAboveThresholdTaskCanTx": {
                "bits": 1
            },
            "WatchdogTimeout": {
                "bits": 1
            }
        }
    },

    "BMSImdStatus": {
            "msg_id": 105,
            "cycle_time": 1000,
            "description": "BMS IMD Messages",
            "signals": {
                "OkHs": {
                    "bits": 1
                },
                "SecondsSincePowerOn" : {
                    "bits": 8,
                    "min": 0,
                    "max": 255,
                    "unit": "Seconds"
                },
                "ValidDutyCycle": {
                    "bits": 1
                }
            }
        },

    "BMSImdData": {
        "msg_id": 106,
        "cycle_time": 1000,
        "description": "BMS IMD Measurement Data Values",
        "signals": {
            "ActiveFrequency" : {
                "bits": 2,
                "min": 0,
                "max": 20,
                "unit": "Hz"
            },
            "InsulationMeasurementDcp10Hz" : {
                "resolution": 0.1,
                "min" : 0,
                "max": 65535,
                "unit": "kOhm"
            },
            "InsulationMeasurementDcp20Hz" : {
                "resolution": 0.1,
                "min" : 0,
                "max": 65535,
                "unit": "kOhm"
            },
            "SpeedStartStatus30Hz":{
                "resolution": 0.1,
                "min": 0,
                "max": 65535
            }
        }
    },

    "BMSImdPwmOutput": {
        "msg_id": 107,
        "cycle_time": 1000,
        "description": "BMS IMD PWM Output",
        "signals": {
            "DutyCycle": {
                "bits": 3,
                "min": 0,
                "max": 100,
                "unit": "%"
            },
            "Frequency": {
                "bits": 4,
                "min": 0,
                "max": 100,
                "unit": "Hz"
            }
        }
    },
    "BMSFaults": {
        "msg_id": 109,
        "cycle_time": 1000,
        "description": "BMS Fault Flags",
        "signals": {
            "ChargerDisconnectedInChargeState": {
                "bits": 1
            },
            "CellUnderVoltageFault": {
                "bits": 1
            },
            "CellOverVoltageFault": {
                "bits": 1
            },
            "ModuleCommFault": {
                "bits": 1
            },
            "CellUnderTempFault":{
                "bits": 1
            },
            "CellOverTempFault": {
                "bits": 1
            },
            "ChargerFault": {
                "bits": 1
            },
            "HasReachedMaxV":{
                "bits": 1
            },
            "ChargingExtShutdownOccurred": {
                "bits": 1
            },
            "TSOverCurrentFault": {
                "bits": 1
            },
            "PreChargeFault": {
                "bits": 1
            }
        }
    },

    "BMSCharger": {
        "msg_id": 110,
        "cycle_time": 1000,
        "description": "Flags relaying BMS Charger Status",
        "signals": {
            "IsConnected": {
                "bits": 1
            },
            "IsChargingComplete": {
                "bits": 1
            }
        }
    },

    "BmsOkStatuses": {
        "msg_id": 111,
        "cycle_time": 10,
        "description": "BMS OK status flags",
        "signals": {
            "BmsOk": {
                "bits": 1
            },
            "ImdOk":{
                "bits": 1
            },
            "BspdOk": {
                "bits": 1
            }
        }
    },

    "BMSAirStates": {
        "msg_id": 112,
        "cycle_time": 10,
        "description": "BMS AIR state statuses",
        "signals": {
            "AirPositive": {
                "bits": 1
            },
            "AirNegative":{
                "bits": 1
            },
            "PrechargeRelay": {
                "bits": 1
            }
        }
    },

    "BMSMotorShutdownErrors": {
        "msg_id": 113,
        "cycle_time": 1000,
        "description": "BMS Motor Shutdown Errors",
        "signals": {
            "BmsDummyMotorShutdown": {
                "bits": 1
            }
        }
    },

    "BMSCellTemperatures": {
        "msg_id": 114,
        "cycle_time": 100,
        "description": "BMS Cell Temperature readings",
        "signals": {
            "MinCellTemperature": {
                "resolution": 0.1,
                "min": 0,
                "max": 100,
                "unit": "degC"
            },
            "MaxCellTemperature": {
                "resolution": 0.1,
                "min": 0,
                "max": 100,
                "unit": "degC"
            }
        }
    },

    "BMSCellVoltages": {
        "msg_id": 115,
        "cycle_time": 100,
        "description": "BMS Cell Voltage readings",
        "signals": {
            "MinCellVoltage": {
                "resolution": 0.0001,
                "min": 0,
                "max": 5,
                "unit": "V"
            },
            "MaxCellVoltage": {
                "resolution": 0.0001,
                "min": 0,
                "max": 5,
                "unit": "V"
            }
        }
    },

    "BMSCellStats": {
        "msg_id": 116,
        "cycle_time": 100,
        "description": "BMS Cell Stats",
        "signals": {
            "MinCellVoltageSegment": {
                "resolution": 1,
                "min": 0,
                "max": 7
            },
            "MinCellVoltageIdx": {
                "resolution": 1,
                "min": 0,
                "max": 15
            },
            "MaxCellVoltageSegment": {
                "bits": 3,
                "min": 0,
                "max": 7
            },
            "MaxCellVoltageIdx":{
                "resolution": 1,
                "min": 0,
                "max": 15
            },
            "MinTempSegment": {
                "resolution": 1,
                "min": 0,
                "max": 7
            },
            "MinTempIdx": {
                "resolution": 1,
                "min": 0,
                "max": 15
            },
            "MaxTempSegment": {
                "resolution": 1,
                "min": 0,
                "max": 7
            },
            "MaxTempIdx":{
                "resolution": 1,
                "min": 0,
                "max": 15
            }
        }
    },

    "BMSTractiveSystem": {
        "msg_id": 117,
        "cycle_time": 100,
        "description": "BMS Tractive System Data",
        "signals": {
            "TsVoltage": {
                "resolution": 0.0001,
                "min": 0,
                "max": 600,
                "unit": "V"
            },
            "TsCurrent": {
                "resolution": 0.1,
                "min": -300,
                "max": 300,
                "unit": "A"
            }
        }
    },

    "BMSPackVoltage": {
        "msg_id": 118,
        "cycle_time": 100,
        "description": "BMS Pack Voltage",
        "signals": {
            "PackVoltage": {
                "resolution": 0.0001,
                "min": 0,
                "max": 600,
                "unit": "V"
            }
        }
    },

    "BMSAvailablePower": {
        "msg_id": 120,
        "cycle_time": 100,
        "description": "BMS Available Power",
        "signals": {
            "AvailablePower": {
                "resolution": 0.1,
                "min": 0,
                "max": 100000,
                "unit": "W"
            }
        }
    }
}
