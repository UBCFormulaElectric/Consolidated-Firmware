                "enum": "ContactorState"
            }
        }
    },
    "BMS_MotorShutdownErrors": {
        "msg_id": 113,
        "cycle_time": 1000,
        "description": "BMS Motor Shutdown Errors",
        "signals": {
            "BmsDummyMotorShutdown": {
                "bits": 1
            }
        }
    },
    "BMS_CellTemperatures": {
        "msg_id": 114,
        "cycle_time": 100,
        "description": "BMS Cell Temperature readings",
        "signals": {
            "MinCellTemperature": {
                "resolution": 0.1,
                "min": 0,
                "max": 100,
                "unit": "degC"
            },
            "MaxCellTemperature": {
                "resolution": 0.1,
                "min": 0,
                "max": 100,
                "unit": "degC"
            }
        }
    },
    "BMS_CellVoltages": {
        "msg_id": 115,
        "cycle_time": 100,
        "description": "BMS Cell Voltage readings",
        "signals": {
            "MinCellVoltage": {
                "resolution": 0.0001,
                "min": 0,
                "max": 5,
                "unit": "V"
            },
            "MaxCellVoltage": {
                "resolution": 0.0001,
                "min": 0,
                "max": 5,
                "unit": "V"
            }
        }
    },
    "BMS_CellStats": {
        "msg_id": 116,
        "cycle_time": 100,
        "description": "BMS Cell Stats",
        "signals": {
            "MinCellVoltageSegment": {
                "resolution": 1,
