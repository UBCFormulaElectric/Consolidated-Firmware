{
    "BMS_Vitals": {
        "msg_id": 100,
        "cycle_time": 100,
        "description": "Vitals message for the BMS.",
        "signals": {
            "Heartbeat": {
                "bits": 1
            },
            "CurrentState": {
                "enum": "BmsState"
            },
            "StateOfCharge": {
                "bits": 3,
                "min": 0,
                "max": 100
            }
        }
    },
    "BMS_CanFifoOverflow": {
        "msg_id": 101,
        "cycle_time": 5000,
        "description": "BMS tx/rx fifo overflow count",
        "signals": {
            "TxOverflowCount": {
                "bits": 32
            },
            "RxOverflowCount": {
                "bits": 32
            }
        }
    },
    "BMS_WatchdogTimeout": {
        "msg_id": 103,
        "cycle_time": 1000,
        "description": "placeholder",
        "signals": {
            "TaskName": {
                "enum": "RtosTaskName"
            }
        }
    },
    "BMS_Warnings": {
        "msg_id": 104,
        "cycle_time": 1000,
        "description": "Warning flags for BMS",
        "signals": {
            "StackWatermarkAboveThresholdTask1Hz": {
                "bits": 1
            },
            "StackWatermarkAboveThresholdTask100Hz": {
                "bits" : 1
            },
            "StackWatermarkAboveThresholdTask1kHz": {
                "bits": 1
            },
            "StackWatermarkAboveThresholdTaskCanRx": {
                "bits": 1
            },
            "StackWatermarkAboveThresholdTaskCanTx": {
                "bits": 1
            },
            "WatchdogTimeout": {
                "bits": 1
            },
            "WatchdogTimeoutTaskName": {
                "enum": "RtosTaskName"
            }
        }
    },
    "BMS_ImdStatus": {
        "msg_id": 105,
        "cycle_time": 1000,
        "description": "BMS IMD Messages",
        "signals": {
            "OkHs": {
                "bits": 1
            },
            "SecondsSincePowerOn": {
                "bits": 8,
                "min": 0,
                "max": 255,
                "unit": "Seconds"
            },
            "ValidDutyCycle": {
                "bits": 1
            },
            "Condition": {
                "bits": 3
            }
        }
    },
    "BMS_ImdData": {
        "msg_id": 106,
        "cycle_time": 1000,
        "description": "BMS IMD Measurement Data Values",
        "signals": {
            "ActiveFrequency": {
                "bits": 2,
                "min": 0,
                "max": 20,
                "unit": "Hz"
            },
            "InsulationMeasurementDcp10Hz": {
                "resolution": 0.1,
                "min": 0,
                "max": 65535,
                "unit": "kOhm"
            },
            "InsulationMeasurementDcp20Hz": {
                "resolution": 0.1,
                "min": 0,
                "max": 65535,
                "unit": "kOhm"
            },
            "SpeedStartStatus30Hz": {
                "resolution": 0.1,
                "min": 0,
                "max": 65535
            }
        }
    },
    "BMS_ImdPwmOutput": {
        "msg_id": 107,
        "cycle_time": 1000,
        "description": "BMS IMD PWM Output",
        "signals": {
            "DutyCycle": {
                "bits": 3,
                "min": 0,
                "max": 100,
                "unit": "%"
            },
            "Frequency": {
                "bits": 4,
                "min": 0,
                "max": 100,
                "unit": "Hz"
            }
        }
    },
    "BMS_Faults": {
        "msg_id": 109,
