                "min": 0,
                "max": 7
            },
            "MaxCellVoltageIdx": {
                "resolution": 1,
                "min": 0,
                "max": 15
            },
            "MinTempSegment": {
                "resolution": 1,
                "min": 0,
                "max": 7
            },
            "MinTempIdx": {
                "resolution": 1,
                "min": 0,
                "max": 15
            },
            "MaxTempSegment": {
                "resolution": 1,
                "min": 0,
                "max": 7
            },
            "MaxTempIdx": {
                "resolution": 1,
                "min": 0,
                "max": 15
            }
        }
    },
    "BMS_TractiveSystem": {
        "msg_id": 117,
        "cycle_time": 100,
        "description": "BMS Tractive System Data",
        "signals": {
            "TsVoltage": {
                "resolution": 0.0001,
                "min": 0,
                "max": 600,
                "unit": "V"
            },
            "TsCurrent": {
                "resolution": 0.1,
                "min": -300,
                "max": 300,
                "unit": "A"
            }
        }
    },
    "BMS_PackStatus": {
        "msg_id": 118,
        "cycle_time": 100,
        "description": "BMS Pack Status",
        "signals": {
            "PackVoltage": {
                "resolution": 0.0001,
                "min": 0,
                "max": 600,
                "unit": "V"
