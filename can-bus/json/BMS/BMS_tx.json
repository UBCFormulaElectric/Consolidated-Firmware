{
    "BMSVitals": {
        "msg_id": 100,
        "cycle_time": 1000,
        "description": "Vitals message for the BMS.",
        "signals": {
            "Heartbeat": {
                "bits": 1
            },
            "CurrentState":{
                "enum" : "BmsState"
            }
        }
    },

    "BMSCanFifoOverflow": {
        "msg_id": 101,
        "cycle_time": 1000,
        "description": "BMS tx/rx fifo overflow count",
        "signals": {
            "TxOverflowCount": {
                "bits": 32
            },
            "RxOverflowCount": {
                "bits": 32
            }
        }
    },

    "BMSWatchdogTimeout": {
        "msg_id": 102,
        "cycle_time": 1000,
        "description": "placeholder",
        "signals": {
            "TaskName" : {
                "enum": "SharedTasks"
            }
        }
    },

    "BMSWarnings": {
        "msg_id" : 103,
        "cycle_time":  1000,
        "description": "Warning flags for BMS",
        "signals": {
            "StackWatermarkAboveThresholdTask1HZ": {
                "bits": 1
            },
            "StackWatermarkAboveThresholdTask1KHZ": {
                "bits": 1
            },
            "StackWatermarkAboveThresholdTaskCanRx": {
                "bits": 1
            },
            "StackWatermarkAboveThresholdTaskCanTx": {
                "bits": 1
            },
            "WatchdogTimeout": {
                "bits": 1
            }
        }
    },

    "BmsImd": {
            "msg_id": 104,
            "cycle_time": 1000,
            "description": "BMS IMD Messages",
            "signals": {
                "SecondsSincePowerOn" : {
                    "bits": 8,
                    "unit": "Seconds"
                },
                "ValidDutyCycle": {
                    "bits": 1
                },
                "InsulationMeasurementDcp10Hz" : {
                    "resolution": 0.1,
                    "min" : 0,
                    "max": 65535,
                    "unit": "kOhm"
                },
                "Insulation_Measurement_DCP_20Hz" :{
                    "resolution": 0.1,
                    "min": 0,
                    "max": 65535,
                    "unit": "kOhm"
                },
                "SpeedStartStatus30Hz":{
                    "resolution": 0.1,
                    "min": 0,
                    "max": 65535
                },
                "OkHs": {
                    "bits": 1
                }
            }
        },

    "BmsCharger": {
        "msg_id": 110,
        "cycle_time": 1000,
        "description": "Flags relaying BMS Charger Status",
        "signals": {
            "IsConnected": {
                "bits": 1
            },
            "IsChargingComplete": {
                "bits": 1
            }
        }
    },

    "BmsOkStatuses": {
        "msg_id": 111,
        "cycle_time": 1000,
        "description": "BMS OK status flags",
        "signals": {
            "BmsOk": {
                "bits": 1
            },
            "ImdOk":{
                "bits": 1
            },
            "BspdOk": {
                "bits": 1
            }
        }
    },

    "BmsAirStates": {
        "msg_id": 112,
        "cycle_time": 1000,
        "description": "BMS AIR state statuses",
        "signals": {
            "AirPositive": {
                "bits": 1
            },
            "AirNegative":{
                "bits": 1
            },
            "PrechargeRelay": {
                "bits": 1
            }
        }
    },

    "BmsMotorShutdownErrors": {
        "msg_id": 113,
        "cycle_time": 1000,
        "description": "BMS Motor Shutdown Errors",
        "signals": {
            "BmsDummyMotorShutdown": {
                "bits": 1
            }
        }
    },

    "BmsCellTempuratures": {
        "msg_id": 114,
        "cycle_time": 1000,
        "description": "BMS Cell Temperature readings",
        "signals": {
            "MinCellTemperature": {
                "resolution": 0.1,
                "min": 0,
                "max": 100,
                "unit": "degC"
            },
            "MaxCellTemperature": {
                "resolution": 0.1,
                "min": 0,
                "max": 100,
                "unit": "degC"
            }
        }
    },

    "BmsCellVoltages": {
        "msg_id": 115,
        "cycle_time": 1000,
        "description": "BMS Cell Voltage readings",
        "signals": {
            "MinCellVoltage": {
                "resolution": 0.0001,
                "min": 0,
                "max": 5,
                "unit": "V"
            },
            "MaxCellVoltage": {
                "resolution": 0.0001,
                "min": 0,
                "max": 5,
                "unit": "V"
            }
        }
    },

    "BmsCellStats": {
        "msg_id": 116,
        "cycle_time": 1000,
        "description": "BMS Cell Stats",
        "signals": {
            "MinCellVoltageSegment": {
                "bits": 3,
                "resolution": 1,
                "min": 0,
                "max": 7
            },
            "MinCellVoltageIdx": {
                "bits": 4,
                "resolution": 1,
                "min": 0,
                "max": 15
            },
            "MaxCellVoltageSegment": {
                "bits": 3,
                "min": 0,
                "max": 7
            },
            "MaxCellVoltageIdx":{
                "bits": 4,
                "resolution": 1,
                "min": 0,
                "max": 15
            },
            "MinTempSegment": {
                "bits": 3,
                "resolution": 1,
                "min": 0,
                "max": 7
            },
            "MinTempIdx": {
                "bits": 4,
                "resolution": 1,
                "min": 0,
                "max": 15
            },
            "MaxTempSegment": {
                "bits": 3,
                "resolution": 1,
                "min": 0,
                "max": 7
            },
            "MaxTempIdx":{
                "bits": 4,
                "resolution": 1,
                "min": 0,
                "max": 15
            }
        }
    },

    "BmsTractiveSystem": {
        "msg_id": 117,
        "cycle_time": 1000,
        "description": "BMS Tractive System Data",
        "signals": {
            "TsVoltage": {
                "resolution": 0.0001,
                "min": 0,
                "max": 600,
                "unit": "V"
            },
            "TsCurrent": {
                "resolution": 0.1,
                "signed": true,
                "min": 0,
                "max": 600,
                "unit": "A"
            }
        }
    },

    "BmsPackVoltage": {
        "msg_id": 118,
        "cycle_time": 1000,
        "description": "BMS Pack Voltage",
        "signals": {
            "PackVoltage": {
                "resolution": 0.0001,
                "min": 0,
                "max": 600,
                "unit": "V",
            }
        }
    },

    "BmsAvailablePower": {
        "msg_id": 120,
        "cycle_time": 1000,
        "description": "BMS Available Power",
        "signals": {
            "AvailablePower": {
                "resolution": 0.1,
                "min": 0,
                "max": 100000,
                "unit": "W"
            }
        }
    }
}
