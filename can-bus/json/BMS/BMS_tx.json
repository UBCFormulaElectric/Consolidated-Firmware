            "CellOverVoltageFault": {
                "bits": 1
            },
            "ModuleCommFault": {
                "bits": 1
            },
            "CellUnderTempFault": {
                "bits": 1
            },
            "CellOverTempFault": {
                "bits": 1
            },
            "ChargerFault": {
                "bits": 1
            },
            "HasReachedMaxV": {
                "bits": 1
            },
            "ChargingExtShutdownOccurred": {
                "bits": 1
            },
            "TSOverCurrentFault": {
                "bits": 1
            },
            "PreChargeFault": {
                "bits": 1
            }
        }
    },
    "BMS_Charger": {
        "msg_id": 110,
        "cycle_time": 1000,
        "description": "Flags relaying BMS Charger Status",
        "signals": {
            "IsConnected": {
                "bits": 1
            },
            "IsChargingComplete": {
                "bits": 1
            }
        }
    },
    "BMS_OkStatuses": {
        "msg_id": 111,
